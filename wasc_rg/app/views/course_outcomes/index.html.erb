<h1>Listing course_outcomes</h1>

<table>
  <thead>
    <tr>
      <th>Course work</th>
      <th>Outcome</th>
      <th>Outcome Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_outcomes.each do |course_outcome| %>
      <tr>
        <td><%= course_outcome.course_work.name %></td>
        <td><%= course_outcome.outcome.objective_letter %></td>
	<td><%= course_outcome.outcome.description %></td>
        <td><%= link_to 'Show', course_outcome %></td>
        <td><%= link_to 'Edit', edit_course_outcome_path(course_outcome) %></td>
        <td><%= link_to 'Destroy', course_outcome, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course outcome', new_course_course_outcome_path %>
