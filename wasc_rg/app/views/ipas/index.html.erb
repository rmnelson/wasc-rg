<h1>Listing ipas</h1>

<table>
  <thead>
    <tr>
      <th>Letter</th>
      <th>Outcome</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ipas.each do |ipa| %>
      <tr>
        <td><%= ipa.letter %></td>
        <td><%= ipa.outcome %></td>
        <td><%= ipa.course %></td>
        <td><%= link_to 'Show', ipa %></td>
        <td><%= link_to 'Edit', edit_ipa_path(ipa) %></td>
        <td><%= link_to 'Destroy', ipa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ipa', new_course_ipa_path %>
